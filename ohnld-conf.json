{
    "common" : {
        "verbose" : "debug"
    },

    "core" : {
        "tx-interval" : "10",
        "validity-time" : "30.0",

        "iface-name" : "eth0",
        "terminal-v4-addr": "192.168.1.1",
        "terminal-bandwith-max": "5000 bit/s",

        "v4-addr" : "224.0.1.0",
        "v4-port" : "31001",
        "v4-ttl"  : "10",
        "v4-out-addr" : "10.10.10.228"
    },

    "terminal-ipc" : {
        "url" : "http://localhost:5180/api/v1/interface",
        "update-interval" : "20"
    },

    "update-ipc" : {
        "max-update-interval" : "5",
        "host"                : "127.0.0.1",
        "url"                 : "/api/v1/underlay-route-full-dynamic",
        "content-type"        : "json",
        "port"                : "16001"
    },

    "network-announcement" : [
        [ "192.168.1.0", "24" ]
    ],

    "terminal-data" : {
        "addr-air-v4" : "192.166.10.10"
    }

}
